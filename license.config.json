{
  "license": "HEADER",
  "ignore": [
    ".git",
    ".github",
    ".gitattributes",
    "**/.gitignore",
    ".babelrc",
    ".nycrc",
    ".npmrc",
    ".npmignore",
    ".tern-project",
    ".eslintignore",
    "**/.cucumber.js",
    "node_modules",
    "packages",
    "softhsm",
    "build.cfg",
    "README.md",
    "CONTRIBUTING.md",
    "DEVELOPERS.md",
    "NOTICES.md",
    "CHARTER.md",
    "ISSUE_TEMPLATE.md",
    "MAINTAINERS.md",
    "CHANGELOG.md",
    "contrib-notes",
    "log",
    "LICENSE",
    "HEADER",
    "NOTICE",
    "docs/",
    "umd/",
    "lib/",
    "test/data/",
    "generators/app/templates",
    "coverage",
    "**/.DS_Store",
    "packages/concerto-core/api.txt",
    "coverage",
    "index.d.ts",
    "packages/concerto-core/introspect/parser.js",
    "node_modules",
    ".nyc-output",
    "**/*.pdf",
    "**/*.cta",
    "**/*.md",
    "**/*.zip",
    "**/*.feature",
    "*.yml",
    "*.yaml",
    "*.tgz"
  ],
   "licenseFormats": {
    "js|njk|pegjs|cto|acl|qry": {
      "prepend": "/*",
      "append": " */",
      "eachLine": {
        "prepend": " * "
      }
    },
    "npmrc|editorconfig|txt": {
      "eachLine": {
        "prepend": "# "
      }
    }
  }
}